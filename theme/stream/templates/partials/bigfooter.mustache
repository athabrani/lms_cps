{{!
    @template theme_boost/bigfooter

    Bigger footer with extra info and social media.

    Example context (json):
    {
        "output": {
            "page_doc_link": "Help and documentation",
            "supportemail": "<a href=\"#\">Contact site support</a>",
            "has_popover_links": true,
            "services_support": "Services and support",
            "login_info": "You are logged in as cute kitten",
            "moodle_release": "90210",
            "leftcolumn": "<ul><li>List item</li></ul>",
            "centercolumn": "<p>lorem ipsum</p>",
            "facebookurl": "https://url.com",
            "instagramurl": "https://url.com",
            "pinteresturl": "https://url.com",
            "youtubeurl": "https://url.com",
            "linkedinurl": "https://url.com",
            "twitterurl": "https://url.com"
        }
    }
}}
    <!-- Grid container -->
  <div class="container-fluid stream-footer footer-dark text-light">
    <!--Grid row-->
    <div class="row m-0">
      <!--Grid column-->
      <div class="col-lg-4 col-md-4">
        {{{leftcolumn}}}
      </div>
      <!--Grid column-->

      <!--Grid column-->
                <div class="col-lg-4 col-md-4">
                {{{centercolumn}}}
                </div>
      <!--Grid column-->

      <!--Grid column-->
      <div class="col-lg-4 col-md-4">
          <br><br>
            <h3 class="-align-center" style="text-align: center;">Kontak Kami</h3>
            <p style="text-align: center;">Telkom University Landmark Tower (11.12)<br>Bandung, Jawa Barat</p>
            <p style="text-align: center;"><i class="fa-solid fa-globe"></i>  <a href="https://cpsrg.org/" target="_blank">https://cpsrg.org/</a><br><span><i class="fa-solid fa-envelope"></i>  <a href="mailto:cpslaboratory@telkomuniversity.ac.id" target="_blank">cpslaboratory@telkomuniversity.ac.id</a></span></p>
      </div>
      <!--Grid column-->
    </div>
    <!--Grid row-->
    <div class="row container-fluid m-0">
      <div style="padding: 1rem;" class="col-lg-12 text-center socialmedia">
        {{#facebookurl}}
        <a href="{{facebookurl}}" aria-label="Facebook"><i class="fa fa-facebook"></i></a>
        {{/facebookurl}}
        {{#instagramurl}}
        <a href="{{instagramurl}}" aria-label="Instagram" style="padding: 1rem;" target="_blank"><i class="fa-brands fa-instagram fa-xl"></i></a>
        {{/instagramurl}}
        {{#twitterurl}}
        <a href="{{twitterurl}}" aria-label="Line" style="padding: 1rem;" target="_blank"><i class="fa-brands fa-line fa-xl"></i></a>
        {{/twitterurl}}
        {{#youtubeurl}}
        <a href="{{youtubeurl}}" aria-label="Youtube"><i class="fa fa-youtube"></i></a>
        {{/youtubeurl}}
        {{#linkedinurl}}
        <a href="{{linkedinurl}}" aria-label="Linkedin" style="padding: 1rem;" target="_blank"><i class="fa-brands fa-linkedin fa-xl"></i></a>
        {{/linkedinurl}}
        {{#pinteresturl}}
        <a href="{{pinteresturl}}" aria-label="Pinterest"><i class="fa fa-pinterest"></i></a>
        {{/pinteresturl}}
      </div>
    </div>
  </div>
  <!-- Grid container -->